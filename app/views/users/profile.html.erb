<% Submission.joins(:user).each do |submission| %>
<% if !submission.submission_code.nil? %>
<%= submission.month.capitalize.to_s+' '+submission.league.capitalize %> Submitted on: <%= submission.created_at %>
<pre>
<code>
<% submission.submission_code.lines do |line| %>
<%= line %>
<% end %>
</code>
</pre>
<% end %>
 <BR>
<% end %>
